#: plugins/check_tcp.c:125
msgid ""
" -s, --send=STRING\n"
"    String to send to the server\n"
" -e, --expect=STRING\n"
"    String to expect in server response\n"
" -q, --quit=STRING\n"
"    String to send server to initiate a clean close of the connection\n"
